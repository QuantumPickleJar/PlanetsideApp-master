<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Ps2Api</AssemblyName>
    <RootNamespace>Ps2Api</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="App.xaml" />
    <None Remove="Pages\CharacterSearchPage.xaml" />
    <None Remove="Pages\DetailedCharacterPage.xaml" />
    <None Remove="Pages\FeedPage.xaml" />
    <None Remove="Pages\FeedPageSelect.xaml" />
    <None Remove="Pages\FisuPage.xaml" />
    <None Remove="Pages\FisuPopPage.xaml" />
