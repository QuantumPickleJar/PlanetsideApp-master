    <None Remove="Pages\Pictures\hexagon.jpg" />
    <None Remove="Pages\Pictures\nc_icon.png" />
    <None Remove="Pages\Pictures\planetside2logo.png" />
    <None Remove="Pages\Pictures\tr_icon.png" />
    <None Remove="Pages\Pictures\vs_icon.png" />
    <None Remove="Pages\SerachPage.xaml" />
    <None Remove="Pages\SettingsPage.xaml" />
    <None Remove="Pictures\hexagon.jpg" />
    <None Remove="Pictures\nc_icon.png" />
    <None Remove="Pictures\planetside2logo.png" />
    <None Remove="Pictures\ps2title.png" />
